<center>
  <h1>LOGIN</h1>

  <%= form_tag :action => 'attempt_login' do %>
  <p class="form-label"><%= label_tag(:username) %></p>
  <%= text_field_tag(:username) %>
  <p class="form-label"><%= label_tag(:password) %></p>
  <%= password_field_tag(:password) %>
  <br><br>
  <%= submit_tag("Log In") %>
  <% end %>
</center>