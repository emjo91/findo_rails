<h1>Edit Owner</h1>

<%= form_for @owner, :url => { :action => "update", :via => :put} do |f| %>
<p><%= f.label "first_name" %></p>
<%= f.text_field ("first_name") %>
<p><%= f.label "last_name" %></p>
<%= f.text_field ("last_name") %>
<p><%= f.label "phone_num" %></p>
<%= f.text_field ("phone_num") %>
<p><%= f.label "email" %></p>
<%= f.text_field ("email") %>
<p><%= f.label "address" %></p>
<%= f.text_field ("address") %>
<p><%= f.label "city" %></p>
<%= f.text_field ("city") %>
<p><%= f.label "state" %></p>
<%= f.text_field ("state") %>
<p><%= f.label "zip" %></p>
<%= f.text_field ("zip") %>
<br><br>
<%= f.submit %>
<% end %>